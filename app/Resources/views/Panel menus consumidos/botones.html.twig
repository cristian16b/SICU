{% block botones %}
<div class="row">
    <div class="col-lg-6 col-md-6" align="center">
        <div class="btn-group" role="group">
            <button class="btn btn-success" 
                    id="boton-resumen-tarjeta"
                    type="button">Ver VENTAS</button>
            <button class="btn btn-default" 
                    type="button" 
                    id="boton-exportar-ventas-pdf"
                    >PDF</button>
            <button class="btn btn-default" 
                    type="button" 
                    id="boton-exportar-ventas-xls"
                    >XLS
            </button>
        </div>
    </div>
    <div class="col-lg-6 col-md-6" align="center">
        <div class="btn-group" role="group">
            <button class="btn btn-warning" 
                    id="boton-resumen-tarjeta"
                    type="button">Ver CONSUMOS</button>
            <button class="btn btn-default" 
                    type="button" 
                    id="boton-exportar-consumos-pdf"
                    >PDF</button>
            <button class="btn btn-default" 
                    type="button" 
                    id="boton-exportar-consumos-xls"
                    >XLS
            </button>
        </div>
    </div>
</div>    
{% endblock %}
