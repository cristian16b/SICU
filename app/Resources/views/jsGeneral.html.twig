                }
                );
        });
    $(function() 
        {
                $('#fecha-inicio').datepicker({ 
                beforeShowDay: noExcursion
                }
                );
        });
    $(function() 
        {
                $('#fecha-fin').datepicker({ 
                beforeShowDay: noExcursion
                }
                );
        });
</script>
    
